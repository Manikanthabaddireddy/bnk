{
	"name": "localmysql_to_gen2",
	"properties": {
		"activities": [
			{
				"name": "localmysql_to_datalake",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 1,
					"retryIntervalInSeconds": 30,
					"secureOutput": true,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "`customers`"
					},
					{
						"name": "Destination",
						"value": "stage/mysqldata/customers"
					}
				],
				"typeProperties": {
					"source": {
						"type": "MySqlSource"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"parallelCopies": 4,
					"dataIntegrationUnits": 16,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "localmysql",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "csvdata",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}