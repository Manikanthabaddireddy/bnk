{
	"name": "DelimitedText2",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"container": {
				"type": "String"
			},
			"folder": {
				"type": "String"
			},
			"file": {
				"type": "String"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().file",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(dataset().folder,utcNow('yyyy-MM-dd'))",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().container",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}